[access]
	inheritFrom = All-Projects
[submit]
[project]
	description = Natural language detection tool. Written in Dart, a derivative of https://github.com/wooorm/franc/
[access "refs/*"]
	owner = group user/svonidze
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group shlang-dev
	label-Code-Review = -2..+2 group user/svonidze
	label-Verified = -1..+1 group user/svonidze
	label-Verified = -1..+0 group shlang-dev
	push = group shlang-dev
	read = group shlang-dev
	rebase = group shlang-dev
	removeReviewer = group shlang-dev
	submit = group shlang-dev
[access "refs/heads/*"]
	create = group user/svonidze
	push = +force group user/svonidze
	pushMerge = group user/svonidze
	read = group shlang-dev
[access "refs/tags/*"]
	push = group user/svonidze
	push = +force group user/svonidze
